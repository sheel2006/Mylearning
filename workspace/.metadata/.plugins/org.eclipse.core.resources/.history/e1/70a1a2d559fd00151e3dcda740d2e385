import java.util.Hashtable;
import java.util.Iterator;

public class Test {
	public static void main(String[] args) {
		
		Hashtable hs= new Hashtable();
		hs.put("A", 10);
		hs.put("A", 20);
		hs.put("C", 40);
		hs.put("D", 30);
		
