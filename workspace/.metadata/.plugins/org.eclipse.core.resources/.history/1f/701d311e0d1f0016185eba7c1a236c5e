package Pratice;

import java.util.ArrayList;

public class ArrayListPract {

	public static void main(String[] args) {
		emp e1 = new emp(10,"TestA");
	ArrayList al = new ArrayList();
	al.add(10);
	al.add("A");
	al.add(e1);
	//System.out.println(al);
	for(Object oj:al){
		if(oj instanceof Integer)
			System.out.println(al.toString());
		if(oj instanceof Character)
			System.out.println(al.toString());
		if(oj instanceof emp){
			emp e =(emp)oj;
			System.out.println(e.empid+e.ename);
		}
	}
	

	}

}
