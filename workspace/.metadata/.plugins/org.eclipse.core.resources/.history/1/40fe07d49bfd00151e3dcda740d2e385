
public class Block {

	Block(){
		System.out.println("Print Construtor");
	}
	Block(int i){
		System.out.println("print construtor with parmeter");
	}
	{
		System.out.println("1st instance block");
	}
	{
		System.out.println("2nd instnace block");
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Block b1= new Block();

	}

}
